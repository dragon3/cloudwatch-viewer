* mysql
dotcloud deploy -t mysql dragon3.cwvdb
dotcloud info dragon3.cwvdb
dotcloud run louis.db -- mysql -uroot \'-p$ROOT_PASSWORD\'
create database cwv;

* pit
ppit set cloudwatchviewer
